{
  "name": "clubpulse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "node utils-patch.js",
    "dev": "next dev --turbopack",
    "dev:regular": "next dev",
    "prebuild": "node utils-patch.js",
    "build": "npx prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "dotenv -e .env.local -- npx prisma generate",
    "prisma:studio": "dotenv -e .env.local -- npx prisma studio",
    "prisma:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
    "prisma:reset": "dotenv -e .env.local -- npx prisma migrate reset",
    "prisma:push": "dotenv -e .env.local -- npx prisma db push"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.8.0",
    "@prisma/client": "^6.6.0",
    "google-auth-library": "^9.15.1",
    "googleapis": "^148.0.0",
    "mjml": "^4.15.3",
    "next": "15.3.0",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "pusher": "^5.2.0",
    "pusher-js": "^8.4.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.2",
    "resend": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/mjml": "^4.7.4",
    "@types/node": "^20",
    "@types/pg": "^8.11.13",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "pg": "^8.14.1",
    "prisma": "^6.6.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
