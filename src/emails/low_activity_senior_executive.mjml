            <mj-button href="https://example.com/project-updates">See Project Updates</mj-button>
            <mj-text padding-top="15px">Regards,<br />The ClubPulse Team</mj-text>
          </mj-column>
        </mj-section>
        <mj-section padding="20px 0"><mj-column><mj-text align="center" color="#6c757d" font-size="12px">Senior Executive Notification</mj-text></mj-column></mj-section>
      </mj-body>
    </mjml>